.common-btn span {
  background-position: 0 -595px;
  height: 31px;
}
.common-btn span span {
  background-position: right -595px;
  line-height: 31px;
  color: white;
  font-weight: bold;
  text-shadow: 1px 1px 0 #B15F15;
  text-align:center;
  padding-left:15px;
}
.ex_signup_box .ex_signup_right {
  width: 440px;
  float: left;
  padding-left: 10px;
  height: 426px;
}
.ex_signup_content {
  padding: 17px 0px 0px 25px;
}
.ex_signup_content h3 {
  font-size: 14px;
  font-weight: bold;
  padding-left: 20px;
  color: #FF6400;
}
.ex_signup_content span p {
  font-size: 14px;
  font-weight: bold;
  height: 60px;
  line-height: 60px;
  color: #666666;
}
.ex_signup_content li {
  list-style: none;
  padding: 7px 0px 7px 10px;
  background: url(../../images/icon.gif) no-repeat 0px 16px;
}
.ex_signup_content li font {
  color: #ff6400;
  font-weight: bold;
  font-size: 12px;
}
.ex_signup_content_btn {
  padding: 10px 0px 0px 15px;
  display: none;
}
.ex_alerttitle span {
  font-size: 14px;
  font-weight: bold;
}
.ex_si_content {
  padding: 5px 0px;
}
.ex_si_content ul li {
  float: left;
  display: block;
  padding: 0px 3px;
  list-style: none;
}
.ex_signup_tail {
  padding: 8px 0px 10px 5px;
  clear: both;
  overflow: hidden;
}