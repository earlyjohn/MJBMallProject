#register_mainbody_box {
  width: 990px;
  margin: 0px auto;
  overflow: hidden;
}
.clearfix {
  display: block;
}
.clearfix:after {
  content: "\0020";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
  overflow: hidden;
}
.ex_signup_box {
  font-size: 12px;
  font-weight: normal;
}
.ex_register_box {
  border: 1px solid #ccc;
  background: url(../../images/login_bg.gif) repeat-x left bottom;
  overflow: hidden;
}
.ex_signup_left {
  width: 520px;
  float: left;
  background: url(../../images/signup_line.gif) no-repeat right center;
}
.ex_sigunp_titile h1 {
  font-size: 14px;
  font-weight: bolder;
  text-align: center;
  padding-right: 160px;
  height: 25px;
  line-height: 50px;
}
/*.login_yellow, .new_deal .OrderList .OrderListName, .g_assemble_title ul li a:link, .g_assemble_title ul li a:visited {
  color: #FF6400;
}*/
.register_box {
  border: 0px;
}
.signupform div {
    border: 0px;
}

.register_box {
  border: 1px solid #ddd;
  padding: 10px;
  margin: 8px 0 12px 0;
  overflow: hidden;
  zoom: 1;
  clear: both;
}
.register_box ul {
  margin: 5px;
  padding: 0;
  list-style-type: none;
}
.register_box li {
  padding: 3px 0;
  clear: both;
  overflow: hidden;
}
.register_box .rleftlabel {
  padding: 5px 2px;
  margin: 0;
  float: left;
  width: 105px;
  text-align: right;
  height: auto;
  font-family: Verdana, Geneva, sans-serif, "宋体";
}
.register_box li .rfloatdiv {
  padding: 5px 2px;
  float: left;
  height: auto;
}
.register_box .rinputstyle {
  width: 160px;
}
.rinputstyle, .rx-input {
  border: 1px solid #ccc;
  padding: 3px 5px;
  background: url(../../images/Numinput-bj.gif) no-repeat 0 0 #fff;
  font-size: 12px;
}
a:link, a:visited, a {
  color: #666666;
  text-decoration: none;
}
