<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	
	<!-- action=searchProduct；类是com.uparttime.searchProduct.controller.searchProductController；方法：searchProduct -->
 	<package name="searchProduct" extends="json-default">
		<action name="searchProduct" class="com.uparttime.searchproduct.controller.SearchProductController" method="searchProduct">
			<result type="json">
			   <param name="noCache">true</param>
			   <param name="contentType">text/html</param>
			</result>
		</action>
		
		
	</package>
</struts>