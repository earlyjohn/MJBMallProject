<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	
	<!-- 资费模块 -->
 	<package name="position" extends="json-default">
		<action name="position" class="com.uparttime.position.controller.positionController" method="position">
			<result type="json">
			   <param name="noCache">true</param>
			   <param name="contentType">text/html</param>
			</result>
		</action>
		<action name="positionDetails" class="com.uparttime.position.controller.positionDetailsController" method="positionDetails">
			<result type="json">
			   <param name="noCache">true</param>
			   <param name="contentType">text/html</param>
			</result>
		</action>
		
		
	</package>
</struts>