<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	
	<!-- 资费模块 -->
 	<package name="register" extends="json-default">
		<action name="register" class="com.uparttime.register.controller.registerController" method="register">
			<result type="json">
			   <param name="noCache">true</param>
			   <param name="contentType">text/html</param>
			   <param name="callbackParameter">callback</param>
			</result>
		</action>
		<action name="testregisterphone" class="com.uparttime.register.controller.registerController" method="testphone">
			<result type="json">
			   <param name="noCache">true</param>
			   <param name="contentType">text/html</param>
			   <param name="callbackParameter">callback</param>
			</result>
		</action>
		
	</package>
</struts>